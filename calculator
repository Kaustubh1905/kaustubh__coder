print("1.addition")
print("2.subtraction")
print("3.multiplication")
print("4.division")
print()
ch=int(input("enter the number between [1,2,3,4]:))
             no1=int(input("enter the first number :"))
             no2=int(input("enter the second number:"))
             print()
             if ch ==1
             add=no1+no2
             print("addition between two numbers is:".add)
             elif ch==2
             sub =no1=no2
             print("subtraction between two numbers is:",sub)
             elif ch==3
             multi=no1*no2
             print("multiplication between two numberas is:",multi)
             elif ch==4
             div =no1/no2
             print("division between two numbers is:",div)
             else
             print("entered something wrong")
